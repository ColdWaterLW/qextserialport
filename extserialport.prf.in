CONFIG += $$QESP_PRF_CONFIG

SAVE_TEMPLATE = \$\$TEMPLATE
TEMPLATE = fakelib
greaterThan(QT_MAJOR_VERSION, 4) {
    #different name for Qt4 and Qt5
    QESP_LIBNAME = \$\$qtLibraryTarget(QtExtSerialPort)
} else {
    QESP_LIBNAME = \$\$qtLibraryTarget(qextserialport)
}
TEMPLATE = \$\$SAVE_TEMPLATE

macx:qesp_mac_framework {
} else {
    LIBS += -l\$\$QESP_LIBNAME
    INCLUDEPATH += \$\$[QT_INSTALL_HEADERS]/QtExtSerialPort
}

!qesp_static:DEFINES += QEXTSERIALPORT_USING_SHARED
