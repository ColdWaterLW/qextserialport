INCLUDEPATH += \$\$[QT_INSTALL_HEADERS]/QtExtSerialPort

SAVE_TEMPLATE = \$\$TEMPLATE
TEMPLATE = fakelib
greaterThan(QT_MAJOR_VERSION, 4) {
    #different name for Qt4 and Qt5
    QEXTSERIALPORT_LIBNAME = \$\$qtLibraryTarget(QtExtSerialPort)
} else {
    QEXTSERIALPORT_LIBNAME = \$\$qtLibraryTarget(qextserialport)
}
TEMPLATE = \$\$SAVE_TEMPLATE

LIBS += -l\$\$QEXTSERIALPORT_LIBNAME

DEFINES += QEXTSERIALPORT_USING_SHARED
